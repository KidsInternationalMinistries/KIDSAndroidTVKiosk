<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude all data from cloud backup -->
        <exclude domain="root" />
        <exclude domain="database" />
        <exclude domain="file" />
        <exclude domain="external" />
        <exclude domain="sharedpref" />
    </cloud-backup>
</data-extraction-rules>